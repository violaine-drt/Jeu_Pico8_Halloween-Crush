--bat
function create_bat()
	bat={}
	for i=1,7 do
		local new_bat={
			x=464,
			y=rnd(130,120,110),
			speed=1+rnd(0.5)
		}
		add (bat,new_bat)
	end
	
	for i=1,3 do
		local new_bat={
			x=464,
			y=rnd(130,120,110),
			speed=2+rnd(0.5)
		}
		add (bat,new_bat)
	end
end

function update_bats()
	for b in all(bat) do
		b.x -=b.speed
		if b.x<-1 then
			b.x=464
			b.y=rnd(130,120,110)
		end
	end
end
