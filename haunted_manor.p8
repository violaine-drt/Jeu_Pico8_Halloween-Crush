function doors()
	open_door_down_left()
	open_door_up_right()
	open_door_down_right()
	open_door_middle()
end

function open_door_down_left()
	if btnp(⬆️) and (collide_map(player,"left",7) or  collide_map(player,"right",7))
				then
				fake_door=true
	end
end

function open_door_up_right()
	if btnp(⬆️) and (collide_map(player,"left",5) or  collide_map(player,"right",5))
				then
			player.x=15
			player.y=15
			music(00,-1)
	end
end

function open_door_down_right()
	if btnp(⬆️) and (collide_map(player,"left",6) or  collide_map(player,"right",6))
				then
			player.x=888
			player.y=75
	end
end

function open_door_middle()
	if player.y<80 and (collide_map(player,"left",4) or  collide_map(player,"right",4)) then
	no_door()
end
end

function no_door()
	mset(888,64,46)
	mset(896,64,46)
	mset(896,72,46)
	mset(888,72,46)
end
